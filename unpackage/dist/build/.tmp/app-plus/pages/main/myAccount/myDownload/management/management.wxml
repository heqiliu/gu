<view class="content"><view class="status-bar"></view><cmd-nav-bar class="nav-bar" vue-id="1" fixed="{{false}}" back background-color="linear-gradient(to right, #EF9435, #E95E28)" title="我的库房" font-color="#fff" bind:__l="__l"></cmd-nav-bar><view class="infoLists"><view class="son"><text>{{toproxyData.todayInviteNumber}}</text><text>今日新增人数</text></view><view data-event-opts="{{[['tap',[['showLower',['$event']]]]]}}" class="son" bindtap="__e"><text></text><text>我的分享</text></view><view class="son"><text>{{toproxyData.groupSize}}</text><text>团队总人数</text></view></view><block wx:if="{{showView}}"><view class="lback"><view class="lcont"><view class="lGroup"><text class="nams">代理用户 :</text><view class="uni-list-cell-db pickers"><picker value="{{index}}" range-key="phone" range="{{lowerList}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{lowerList[index].phone+' '+lowerList[index].name}}</view></picker></view><text class="bfb"></text></view><view class="lGroup"><text class="nams">代理抽成 :</text><input type="number" value="{{lowerList[index].deferredCommission}}" data-event-opts="{{[['input',[['__set_model',['$0','deferredCommission','$event',[]],['lowerList.'+index+'']]]]]}}" bindinput="__e"/><text class="bfb">%</text></view><view data-event-opts="{{[['tap',[['destroyMember',['$event']]]]]}}" class="isBtn" bindtap="__e">确认分配</view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="closeview" bindtap="__e">X</view></view></view></block></view>
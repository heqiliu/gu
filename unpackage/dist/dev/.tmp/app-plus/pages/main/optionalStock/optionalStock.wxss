.status-bar.data-v-350a48fc {
  box-sizing: border-box;
  display: block;
  width: 100%;
  margin-bottom: -3rpx;
  height: var(--status-bar-height);
  line-height: var(--status-bar-height);
  position: fixed;
  top: 20rpx;
  left: 0;
  background: -webkit-linear-gradient(left, #EF9435, #E95E28);
  background: linear-gradient(to right, #EF9435, #E95E28);
  z-index: 99;
}
.nav-bar.data-v-350a48fc {
  position: fixed;
  top: var(--status-bar-height);
